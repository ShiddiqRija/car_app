<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>
    
        <!-- View -->

        <!-- Configuration View -->
        <record id="car_vehicle_service_type_view_tree" model="ir.ui.view">
            <field name="name">car.vehicle.service.type.view.tree</field>
            <field name="model">car.service.type</field>
            <field name="arch" type="xml">
                <tree string="Service Type" editable="top">
                    <field name="name" />
                </tree>
            </field>
        </record>

        <!-- Action -->

        <!-- Configuration Action -->
        <record id="car_vehicle_service_type_action" model="ir.actions.act_window">
            <field name="name">Service Type</field>
            <field name="res_model">car.service.type</field>
            <field name="view_mode">tree,form</field>
        </record>
    
    </data>

</odoo>
